{
  "name": "@forex-marketplace/forex-marketplace-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "forex-marketplace-e2e",
    "implicitDependencies": [
      "forex-marketplace"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/forex-marketplace-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "forex-marketplace:build"
        ]
      }
    }
  }
}
